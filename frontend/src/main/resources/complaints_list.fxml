<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox prefHeight="400.0" prefWidth="855.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <MenuBar>
        <menus>
          <Menu mnemonicParsing="false" text="Back">
            <items>
              <MenuItem mnemonicParsing="false" onAction="#close" text="Close" />
            </items>
          </Menu>
        </menus>
      </MenuBar>
      <TableView fx:id="table" prefHeight="425.0" prefWidth="600.0">
        <columns>
          <TableColumn fx:id="userCol" prefWidth="139.0" text="User" />
          <TableColumn fx:id="descCol" prefWidth="319.0" text="Description" />
            <TableColumn fx:id="dateCol" minWidth="0.0" prefWidth="396.0" text="Date" />
        </columns>
      </TableView>
   </children>
</VBox>
